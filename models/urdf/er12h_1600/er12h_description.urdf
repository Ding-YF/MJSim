      <material name="gray" />
    </visual>
  </link>

  <!-- 关节6: link5 -> link6 -->
  <joint name="joint6" type="revolute">
    <parent link="link5" />
    <child link="link6" />
    <origin xyz="0.075 0 0" rpy="0 0 0" />
    <axis xyz="-1 0 0" />
    <limit lower="-3.05433" upper="3.05433" effort="1000" velocity="1000" />
  </joint>

</robot>